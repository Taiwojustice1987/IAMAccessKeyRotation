{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowLambdaPutObject",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::626635400294:role/IAMKeyRotationRole"
      },
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::my-key-rotation-reports-central",
        "arn:aws:s3:::my-key-rotation-reports-central/*"
      ]
    }
  ]
}
